{
  "name": "rar-stream",
  "version": "3.1.1",
  "description": "Rar-Stream is a module for managing rar archives as node streams",
  "author": "1313",
  "repository": {
    "type": "git",
    "url": "https://github.com/1313/rar-stream.git"
  },
  "type": "module",
  "packageManager": "pnpm@7.18.2",
  "engines": {
    "node": ">=18"
  },
